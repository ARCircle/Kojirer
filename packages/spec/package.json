{
  "name": "spec",
  "version": "0.0.1",
  "main": "index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "require": "./dist/types.js",
      "import": "./dist/types.mjs"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "require": "./dist/schema.js",
      "import": "./dist/schema.mjs"
    }
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "openapi-typescript": "^7.8.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pnpm run clean && pnpm run codegen && pnpm run build:tsup",
    "build:tsup": "tsup-node --config tsup.config.ts",
    "clean": "rm -rf dist && rm -rf ./src/gen",
    "codegen": "pnpm run codegen:openapi-ts",
    "codegen:openapi-ts": "tsx ./src/transform.ts",
    "docs:validate": "pnpm run docs:validate:openapi && pnpm run docs:validate:asyncapi",
    "docs:build": "pnpm run docs:build:openapi && pnpm run docs:build:asyncapi",
    "docs:validate:openapi": "pnpm --package=@redocly/cli dlx redocly lint openapi/openapi.yml",
    "docs:validate:asyncapi": "pnpm dlx @asyncapi/cli validate asyncapi/asyncapi.yml",
    "docs:build:openapi": "pnpm --package=@redocly/cli dlx redocly build-docs openapi/openapi.yml --output docs/openapi/index.html",
    "docs:build:asyncapi": "pnpm dlx @asyncapi/cli generate fromTemplate asyncapi/asyncapi.yml @asyncapi/html-template@3.0.0 --use-new-generator --output docs/asyncapi",
    "docs:serve:openapi": "pnpm --package=@redocly/cli dlx redocly preview-docs openapi/openapi.yml"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
